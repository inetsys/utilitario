{
  "name": "utilitario",
  "version": "0.1.15",
  "description": "Bulletproof identification (is), validation (constraints) and casting (cast)",
  "main": "./index.js",
  "keywords": [],
  "directories": {
    "lib": ".",
    "test": "test"
  },
  "scripts": {
    "test": "tap test/test.js",
    "coverage": "node test/code-coverage.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/llafuente/utilitario.git"
  },
  "author": {
    "name": "Luis Lafuente",
    "email": "llafuente@noboxout.com",
    "url": "http://www.noboxout.com"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.6"
  },
  "dependencies": {
    "qs": "*",
    "is-html": "~0.1.1",
    "lodash.clonedeep": "^2.4.1"
  },
  "devDependencies": {
    "tap": "~0.4.4",
    "ass": "0.0.4",
    "cli-table": "*"
  },
  "jshintConfig": {
    "white": true,
    "curly": true,
    "eqeqeq": true,
    "indent": 4,
    "asi": false,
    "boss": false,
    "browser": true,
    "devel": true,
    "maxdepth": 4,
