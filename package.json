    "maxcomplexity": 6,
    "eqnull": true,
    "es5": true,
    "evil": false,
    "immed": false,
    "jquery": true,
    "latedef": false,
    "laxcomma": true,
    "laxbreak": true,
    "newcap": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "predef": [
      "log"
    ],
    "quotmark": "double",
    "smarttabs": false,
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
